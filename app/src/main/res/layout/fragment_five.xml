<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bitmap3"
    tools:context=".homepage.fragment.fivefragment.FiveFragment">

    <TextView
        android:id="@+id/imagetext"
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/layout" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/mypleasureimage"
        android:layout_width="100dp"
        android:layout_height="100dp"
        fresco:roundAsCircle="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="400dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/tou" />

    <TextView
        android:id="@+id/sistem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="我是吃不胖的小姐姐"
        android:textColor="#000"
        android:layout_marginBottom="330dp"
        android:layout_alignParentBottom="true"
        android:textSize="20dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="230dp"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/my_icon_information_n_hdpi"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:layout_marginLeft="25dp"
            android:paddingRight="30dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/datum"
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textSize="20dp"
                android:text="个人资料"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#a4a08c"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="190dp"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/my_icon_circle_n_hdpi"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:layout_marginLeft="25dp"
            android:paddingRight="30dp"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textSize="20dp"
                android:text="我的圈子"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#a4a08c"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="150dp"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/my_icon_foot_n_hdpi"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:layout_marginLeft="25dp"
            android:paddingRight="30dp"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textSize="20dp"
                android:text="我的足迹"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#a4a08c"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="110dp"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/my_icon_wallet_n_hdpi"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:layout_marginLeft="25dp"
            android:paddingRight="30dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/qianbaofive"
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textSize="20dp"
                android:text="我的钱包"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#a4a08c"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingLeft="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="70dp"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/my_icon_address_n_hdpi"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:layout_marginLeft="25dp"
            android:paddingRight="30dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/address"
                android:layout_width="wrap_content"
                android:layout_height="28dp"
                android:textSize="20dp"
                android:text="我的收货地址"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#a4a08c"/>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>